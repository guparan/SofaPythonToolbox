# CMake package configuration file for the plugin @PROJECT_NAME@

@PACKAGE_GUARD@
@PACKAGE_INIT@

set(SOFAPYTHONTOOLBOX_HAVE_SOFAPYTHON @SOFAPYTHONTOOLBOX_HAVE_SOFAPYTHON@)
set(SOFAPYTHONTOOLBOX_HAVE_SOFAPYTHON3 @SOFAPYTHONTOOLBOX_HAVE_SOFAPYTHON3@)

find_package(SofaFramework QUIET REQUIRED)

if(SOFAPYTHONTOOLBOX_HAVE_SOFAPYTHON)
    find_package(SofaPython QUIET REQUIRED)
endif()
if(SOFAPYTHONTOOLBOX_HAVE_SOFAPYTHON3)
    find_package(SofaPython3 QUIET REQUIRED)
endif()

if(NOT TARGET @PROJECT_NAME@)
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
endif()

check_required_components(@PROJECT_NAME@)